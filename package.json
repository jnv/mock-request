{
  "name": "@codeceptjs/mock-request",
  "version": "0.3.0",
  "description": "Mocking requests with PollyJS for CodeceptJS",
  "main": "index.js",
  "scripts": {
    "publish": "npm run docs & npm publish --access public",
    "test": "npx codeceptjs run -c test/codecept.puppeteer.conf.js --debug --grep @Puppeteer",
    "test-webdriver": "npx codeceptjs run -c test/codecept.webdriver.conf.js --debug --grep @WebDriver",
    "docs": "npx documentation readme index.js --section=API --shallow"
  },
  "keywords": [
    "mock",
    "pollyjs",
    "codeceptjs",
    "http-mock"
  ],
  "author": "Michael Bodnarchuk @davert",
  "license": "ISC",
  "devDependencies": {
    "codeceptjs": "^3.0.3",
    "cross-spawn": "^7.0.3",
    "documentation": "^13.1.0",
    "puppeteer": "^5.5.0",
    "rimraf": "^3.0.2",
    "webdriverio": "^6.10.11"
  },
  "dependencies": {
    "@pollyjs/adapter-puppeteer": "^5.1.0",
    "@pollyjs/core": "^5.1.0",
    "@pollyjs/persister-fs": "^5.0.0"
  }
}
